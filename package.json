{
  "name": "convertly_app",
  "main": "expo-router/entry",
  "version": "8.0.0",
  "scripts": {
    "start": "expo start",
    "build:android": "eas build --platform android --profile preview",
    "offline": "expo start --offline",
    "publish:expo": "eas update --branch preview",
    "build:web": "npx expo export --platform web",
    "release:web": "eas deploy --prod",
    "upgrade": "bun install expo@latest && bunx expo install --fix",
    "android": "bunx expo run:android --device --variant debug",
    "release:android": "bunx expo run:android --device --variant Release",
    "release:ios": "bunx expo run:ios --configuration Release",
    "test": "jest --watchAll",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "ios": "expo run:ios",
    "analyze:web": "source-map-explorer \"dist/_expo/static/js/web/*.js\" \"dist/_expo/static/js/web/*.js.map\" --only-mapped --no-border-checks",
    "analyze:ios": "source-map-explorer \"dist/_expo/static/js/ios/*.js\" \"dist/_expo/static/js/ios/*.js.map\" --only-mapped --no-border-checks",
    "analyze:android": "source-map-explorer \"dist/_expo/static/js/android/*.js\" \"dist/_expo/static/js/android/*.js.map\" --only-mapped --no-border-checks"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/metro-runtime": "~55.0.6",
    "@expo/vector-icons": "^15.0.3",
    "@sentry/react-native": "~7.11.0",
    "expo": "^55.0.0-preview.11",
    "expo-application": "~55.0.6",
    "expo-background-task": "~55.0.6",
    "expo-build-properties": "~55.0.7",
    "expo-constants": "~55.0.5",
    "expo-device": "~55.0.7",
    "expo-font": "~55.0.4",
    "expo-intent-launcher": "~55.0.6",
    "expo-linking": "~55.0.4",
    "expo-notifications": "~55.0.7",
    "expo-router": "~55.0.0-preview.8",
    "expo-splash-screen": "~55.0.6",
    "expo-status-bar": "~55.0.3",
    "expo-task-manager": "~55.0.6",
    "expo-updates": "~55.0.8",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-native": "0.83.2",
    "react-native-country-flag": "^2.0.2",
    "react-native-edge-to-edge": "^1.7.0",
    "react-native-gesture-handler": "~2.30.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-mmkv": "^4.1.2",
    "react-native-nitro-modules": "^0.33.9",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.23.0",
    "react-native-web": "^0.21.0",
    "source-map-explorer": "^2.5.3",
    "vexo-analytics": "^1.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@types/jest": "29.5.14",
    "@types/react": "~19.2.10",
    "@types/react-test-renderer": "^19.1.0",
    "eas-build-cache-provider": "^16.4.2",
    "jest": "~29.7.0",
    "jest-expo": "~55.0.6",
    "react-test-renderer": "19.1.1",
    "typescript": "~5.9.2"
  },
  "private": true,
  "trustedDependencies": [
    "@sentry/cli",
    "unrs-resolver"
  ],
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "enabled": true,
        "exclude": [
          "react-native-keyboard-aware-scroll-view",
          "source-map-explorer",
          "vexo-analytics"
        ],
        "listUnknownPackages": true
      }
    }
  }
}
